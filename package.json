{
  "name": "nog_api",
  "version": "0.1.0",
  "main": "dist/app.js",
  "repository": "https://github.com/gaugau3000/nog_api",
  "author": "Gautier Pialat",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/server.js\"",
    "dev_start": "tsc && node dist/server.js",
    "test": " NODE_ENV=test jest --verbose",
    "test-watch": "NODE_ENV=test jest --verbose --watchAll",
    "start": "node dist/server.js",
    "build": "tsc && yarn run copy:mail_templates",
    "copy:mail_templates": "cpx 'src/mail_templates/**' 'dist/mail_templates'",
    "format": "prettier src/**/*.ts --write",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "concurrently": "^5.1.0",
    "cpx": "^1.5.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/mjml": "^4.0.4",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.21",
    "mjml": "^4.6.3",
    "nodemailer": "^6.4.6",
    "vue": "^2.6.11",
    "vue-server-renderer": "^2.6.11"
  }
}
